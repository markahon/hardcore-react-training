{
  "presets": [
    [
      "@babel/preset-env",
      {
        "ignoreBrowserslistConfig": true,
        "debug": false,
        "targets": {
          "node": "current"
        },
        "modules": "commonjs"
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", {"loose": true}],
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-syntax-dynamic-import"
  ]
}
